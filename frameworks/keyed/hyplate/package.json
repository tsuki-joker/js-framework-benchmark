{
  "name": "js-framework-benchmark-hyplate",
  "version": "1.0.0",
  "type": "module",
  "description": "Benchmark for hyplate framework",
  "js-framework-benchmark": {
    "frameworkVersionFromPackage": "hyplate",
    "frameworkHomeURL": "https://github.com/DarrenDanielDay/hyplate",
    "issues": [
      1
    ]
  },
  "scripts": {
    "build-dev": "esbuild ./index.ts --bundle --format=esm --tsconfig=./tsconfig.json --sourcemap --platform=node --watch --define:process.env.NODE_ENV='development' --outfile=dist/main.js",
    "build-prod": "esbuild ./index.ts --bundle --format=esm --tsconfig=./tsconfig.json --minify --outfile=dist/main.js"
  },
  "keywords": [
    "hyplate"
  ],
  "author": "Darren Daniel Day",
  "license": "Apache-2.0",
  "homepage": "https://github.com/krausest/js-framework-benchmark",
  "repository": {
    "type": "git",
    "url": "https://github.com/krausest/js-framework-benchmark.git"
  },
  "dependencies": {
    "hyplate": "^0.1.4"
  },
  "devDependencies": {
    "esbuild": "^0.16.4",
    "typescript": "^4.9.4"
  }
}
